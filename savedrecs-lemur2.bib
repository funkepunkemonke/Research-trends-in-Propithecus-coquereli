@article{ WOS:000073561900031,
Author = {Hafen, T and Neveu, H and Rumpler, Y and Wilden, I and Zimmermann, E},
Title = {Acoustically dimorphic advertisement calls separate morphologically and
   genetically homogenous populations of the grey mouse lemur (Microcebus
   murinus)},
