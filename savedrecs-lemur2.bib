@article{ WOS:000268148900004,
Author = {Raharison, Fidiniaina and Mogicato, Giovanni and Sautet, Jean},
Title = {Anatomy and Ultrasonography of the Normal Kidney in Brown Lemurs:
   Eulemur fulvus},
Journal = {AMERICAN JOURNAL OF PRIMATOLOGY},
Year = {2009},
Volume = {71},
Number = {8},
Pages = {647-653},
Month = {AUG},
Abstract = {The purpose of this study is to describe the anatomy and obtain
   echographic measurements of normal kidneys in brown lemurs (Eulemur
   fulvus). The anatomical findings show that brown lemur kidneys are
   comparable to those of rats except for an elongated papilla. The kidneys
   of 16 (7 females and 9 males) lemurs were examined with two-dimensional
   and power Doppler ultrasonography under general anesthesia.
   Morphometrically, the left and right kidney surface areas are comparable
   (3.29 and 3.51 cm(2)). Kidney area has a significant linear correlation
   with body weight. Echo-Doppler findings show that the mean renal
   arterial blood flow speeds for the left and right kidneys are comparable
   (0.70 and 0.73m/s). However, flow speed is higher in the male (0.79m/s)
   than in the female (0.60m/s). The renal arterial diameters are between
   1.0 and 1.8 mm. The fact that anesthesia can have hemodynamic effects on
   renal vasculature should be taken into consideration when assessing
   these echographic results. Am. J. Primatol. 71:647-653, 2009. (C) 2009
   Wiley-Liss, Inc.},
