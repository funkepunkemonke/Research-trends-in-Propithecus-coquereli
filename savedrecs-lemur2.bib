@incollection{ WOS:000329209700021,
Author = {Cameron, Alex and Gould, Lisa},
Editor = {Marsh, LK and Chapman, CA},
Title = {Fragment-Adaptive Behavioural Strategies and Intersite Variation in the
   Ring-Tailed Lemur (Lemur catta) in South-Central Madagascar},
Booktitle = {PRIMATES IN FRAGMENTS: COMPLEXITY AND RESILIENCE},
Series = {Developments in Primatology-Progress and Prospects},
Year = {2013},
Pages = {227-243},
Abstract = {Behaviour, diet and population demography were sampled and compared
   between two forest fragment-living populations of wild ringtailed lemurs
   (Lemur catta) in south-central Madagascar. Both sites-a fragment in the
   Tsaranoro Valley near Andringitra National Park, and a more densely
   populated fragment at Anja, much closer to human habitation-are sacred
   forests (sites of human burial) surrounded by anthropogenically produced
   savannah, and are subject to traditional protective prohibitions (fady).
   Both sites attract tourists, but are operated differently, with Anja
   receiving considerably more tourists; the resources available to the L.
   catta also differ at each site, affecting their behaviour. L. catta at
   Tsaranoro spent more time feeding, and less time resting and engaging in
   social behaviour than those at Anja, where abundant fruit from
   introduced trees, as well as plentiful drinking water, are available and
   resource abundance is relatively higher. Although the fragments are of
   similar size and were expected to differ little, many significant
   behavioural and population differences were observed, suggesting the
   importance of the refinement of rapid assessment techniques for judging
   the habitat suitability and conservation value of small forest
   fragments.},
