@article{ WOS:000361803500005,
Author = {Gardner, Charlie J. and Jasper, Louise D.},
Title = {Discovery of an island population of dwarf lemurs (Cheirogaleidae:
   Cheirogaleus) on Nosy Hara, far northern Madagascar},
Journal = {PRIMATES},
Year = {2015},
Volume = {56},
Number = {4},
Pages = {307-310},
Month = {OCT},
Abstract = {The species-level diversity of Madagascar's lemurs has increased hugely
   over the last two decades, growing from 32 species in 1994 to 102
   species in 2014. This growth is primarily due to the application of
   molecular phylogenetic analyses and the phylogenetic species concept to
   known populations, and few previously unknown lemur populations have
   been discovered during this time. We report on a new population of dwarf
   lemurs (Cheirogaleus sp.) from Nosy Hara, a 312-ha island in far
   northern Madagascar, which constitutes the northernmost distribution
   record for the genus. The dwarf lemurs appeared to show two
   characteristics of island populations-insular dwarfism and predator na <
   vet,-that suggest a long isolation, and may thus represent an
   undescribed taxon. If this is the case, the dwarf lemurs of Nosy Hara
   are probably one of the rarest primate taxa on Earth.},
