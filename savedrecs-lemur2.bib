Address = {ONE NEW YORK PLAZA, SUITE 4600, NEW YORK, NY, UNITED STATES},
Type = {Article},
Language = {English},
Affiliation = {Ramananjato, V (Corresponding Author), Univ Antananarivo, Ment Zool \& Biodiversite Anim, Antananarivo, Madagascar.
   Ramananjato, Veronarindra, Univ Antananarivo, Ment Zool \& Biodiversite Anim, Antananarivo, Madagascar.
   Razafindratsima, Onja H., Univ Calif Berkeley, Dept Integrat Biol, Berkeley, CA 94720 USA.},
DOI = {10.1007/s10764-021-00224-4},
EarlyAccessDate = {AUG 2021},
