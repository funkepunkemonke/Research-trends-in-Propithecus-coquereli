@article{ WOS:000362238900008,
Author = {Hope, Katharine L. and Boedeker, Nancy C. and Gordon, Sebastian S. and
   Walsh, Timothy F.},
Title = {Solitary Osteochondroma in a Ring-Tailed Lemur (Lemur catta)},
Journal = {COMPARATIVE MEDICINE},
Year = {2015},
Volume = {65},
Number = {4},
Pages = {348-351},
Month = {AUG},
Abstract = {A 20-y-old, male, ring-tailed lemur (Lemur catta) presented with a
   large, firm mass on the proximal caudolateral left femur. The animal
   displayed no clinical signs associated with the mass. Radiographs
   revealed a mineralized mass protruding from the femur, with an intact
   femoral cortex. Histopathology diagnosed osteochondroma in view of the
   presence of a peripheral layer of cartilage with progressive
   endochondral ossification and typical remodeling of bony trabeculae. The
   mass grew quickly after the initial biopsy, and a second surgery to
   debulk 95\% of the tumor was performed. Histopathologic features of the
   larger samples were similar to those of the initial biopsies, with the
   cartilage layer being discontinuous and development of bone from some
   borders progressing directly from a periost-like layer. Nineteen months
   after the second surgery, the mass had regrown and extended further
   proximally on the femur toward the epiphysis, but the animal remained
   asymptomatic, and additional debulking was not attempted. This report is
   the first description of an osteochondroma in a prosimian and describes
   unique behavior of the tumor compared with osteochondromas found in
   humans, dogs, and cats.},
