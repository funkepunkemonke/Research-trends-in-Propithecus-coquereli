@article{ WOS:A1991GF68100008,
Author = {MILLIKEN, GW and STAFFORD, DK and DODSON, DL and PINGER, CD and WARD, JP},
Title = {ANALYSES OF FEEDING LATERALIZATION IN THE SMALL-EARED BUSH-BABY
   (OTOLEMUR-GARNETTII) - A COMPARISON WITH THE RING-TAILED LEMUR
   (LEMUR-CATTA)},
Journal = {JOURNAL OF COMPARATIVE PSYCHOLOGY},
Year = {1991},
Volume = {105},
Number = {3},
Pages = {274-285},
Month = {SEP},
Abstract = {Feeding related lateralization was examined in a population of 23
   small-eared bushbabies (Otolemur garnettii). The three measures used to
   determine lateralization were food reaching, holding, and manipulation.
   Sex and age differences were found, with adult females showing a strong
   right bias and adult males a left bias. Juvenile males were weakly
   lateralized and less consistent across measures than adult animals. The
   use of standard scores to assess lateralization allowed species
   comparisons to be made. The results of this study were compared with
   results from a previous study on lateralization in the ring-tailed lemur
   (Lemur catta). Species comparisons found sex differences to be a
   stronger factor in lateralization than species differences.},
