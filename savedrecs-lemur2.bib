@article{ WOS:000315519000029,
Author = {Bertrand, Anne and Pasquier, Adrien and Petiet, Alexandra and Wiggins,
   Christopher and Kraska, Audrey and Joseph-Mathurin, Nelly and Aujard,
   Fabienne and Mestre-Frances, Nadine and Dhenain, Marc},
Title = {Micro-MRI Study of Cerebral Aging: Ex Vivo Detection of Hippocampal
   Subfield Reorganization, Microhemorrhages and Amyloid Plaques in Mouse
   Lemur Primates},
Journal = {PLOS ONE},
Year = {2013},
Volume = {8},
Number = {2},
Month = {FEB 27},
Abstract = {Mouse lemurs are non-human primate models of cerebral aging and
   neurodegeneration. Much smaller than other primates, they recapitulate
   numerous features of human brain aging, including progressive cerebral
   atrophy and correlation between regional atrophy and cognitive
   impairments. Characterization of brain atrophy in mouse lemurs has been
   done by MRI measures of regional CSF volume and by MRI measures of
   regional atrophy. Here, we further characterize mouse lemur brain aging
   using ex vivo MR microscopy (31 mu m in-plane resolution). First, we
   performed a non-biased, direct volumetric quantification of dentate
   gyrus and extended Ammon's horn. We show that both dentate gyrus and
   Ammon's horn undergo an age-related reorganization leading to a growth
   of the dentate gyrus and an atrophy of the Ammon's horn, even in the
   absence of global hippocampal atrophy. Second, on these first MR
   microscopic images of the mouse lemur brain, we depicted cortical and
   hippocampal hypointense spots. We demonstrated that their incidence
   increases with aging and that they correspond either to amyloid deposits
   or to cerebral microhemorrhages.},
