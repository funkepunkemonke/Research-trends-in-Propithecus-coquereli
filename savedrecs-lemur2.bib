@article{ WOS:A1993KL80800008,
Author = {RENDALL, D},
Title = {DOES FEMALE SOCIAL PRECEDENCE CHARACTERIZE CAPTIVE AYE-AYES
   (DAUBENTONIA-MADAGASCARIENSIS)},
Journal = {INTERNATIONAL JOURNAL OF PRIMATOLOGY},
Year = {1993},
Volume = {14},
Number = {1},
Pages = {125-130},
Month = {FEB},
Abstract = {Several potential measures of dyadic social dominance among captive
   aye-ayes, housed in male-female pairs, were taken for an initial
   description of the social relations. Results suggest that female
   aye-ayes may demonstrate priority in social as well as feeding contexts,
   thus conforming to a social pattern that is characteristic of several
   other Lemuriformes but uncommon among other primates. However, the
   observed precedence of females was not complete or unambiguous.
   Intensive naturalistic study is needed to confirm these observations and
   to clarify the conditions under which this system has evolved among the
   lemuriform primates.},
