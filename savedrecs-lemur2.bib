@article{ WOS:000237352700003,
Author = {Llanos, C and Quintero, G and Castellanos, A and Arevalo-Herrera, M and
   Herrera, S},
Title = {Surgical bone marrow aspiration in Aotus lemurinus griseimembra},
Journal = {JOURNAL OF MEDICAL PRIMATOLOGY},
Year = {2006},
Volume = {35},
Number = {3},
Pages = {131-135},
Month = {JUN},
Abstract = {Aotus lemurinus griseimembra are highly susceptible to infection by
   human malaria parasites and reproduce some of its clinical
   manifestations, including anemia. We developed a new surgical technique
   to obtain bone marrow samples from Aotus by surgical aspiration of the
   femur. First, we determined that the femur offered advantages over other
   bones, primarily due to lower fracture vulnerability. We tested a
   surgical technique using 20 G IV catheters in formaldehyde-preserved
   animals, then conducted the procedure on 27 live animals. This technique
   provided easy, quick surgical access to adequate volumes of bone marrow
   and was safe for almost all animals: only one died; another developed
   nervous impairment of the lower limb. Adequate cell samples were
   obtained in all animals and allowed cytological studies. This procedure
   offers a useful tool for bone marrow research in Aotus and helps
   overcome current limitations of such research in human where these
   studies are limited by ethical and technical issues.},
