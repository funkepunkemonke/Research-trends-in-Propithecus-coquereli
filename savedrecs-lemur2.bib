@article{ WOS:000431408200001,
Author = {Bochkov, Andre V.},
Title = {Gaudalges averianovi sp nov (Acariformes: Psoroptidae)-a new parasite of
   Malagasy lemurs},
Journal = {SYSTEMATIC AND APPLIED ACAROLOGY},
Year = {2018},
Volume = {23},
Number = {4},
Pages = {593-596},
Month = {APR},
Abstract = {The psoroptid subfamily Makialginae (Acari: Psoroptidae) includes six
   genera and 11 species (Bochkov \& OConnor 2006; Bochkov et al. 2010).
   All of them are permanent, highly specialized ectoparasites of
   strepsirrhine primates (Primates: Strepsirrhini) (Bochkov et al. 2011).
   Among them the genus GaudalgesFain, 1963 includes four species
   associated with Malagasy lemurs of the families Indriidae and Lemuridae
   (Bochkov \& OConnor 2006). In this work, I describe a new species
   Gaudalges averianovi sp. nov. from Malagasy lemur Lepidolemur
   ruficaudatus Grandidier (Primates: Lepilemuridae) and provide a key to
   all known species of the genus. This is the first finding of mites of
   the genus Gaudalges on lemurs of the family Lepilemuridae.
   Mites were mounted on slides in Hoyer medium. Drawings of mites were
   made with a Leica microscope equipped with Nomarsky contrast and a
   camera lucida. In the descriptions below, idiosomal chaetotaxy follows
   Griffiths et al. (1990) with modifications of Norton (1998) for coxal
   setae. Leg chaetotaxy follows Grandjean (1941). All measurements in
   descriptions and the key are given in micrometres (mu m) and were taken
   as follows: body length, the total length from the palpal extremities to
   the posterior margin of the body; body width, maximum width taken at
   whatever level it occurs. Host systematics follows Groves (2005).},
Publisher = {SYSTEMATIC \& APPLIED ACAROLOGY SOC LONDON, NATURAL HISTORY MUSEUM},
Address = {DEPT ENTOMOLOGY, LONDON, SW7 5BD, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {Bochkov, AV (Corresponding Author), Russian Acad Sci, Zool Inst, Univ Skaya Embankment 1, St Petersburg 199034, Russia.
