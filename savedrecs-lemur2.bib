@article{ WOS:000399571600007,
Author = {Sarma, Himakshi and Mattaparthi, Venkata Satish Kumar},
Title = {Unveiling the Transient Protein-Protein Interactions that Modulate the
   Activity of Estrogen Receptor(ER)-alpha by Human Lemur Tyrosine Kinase-3
   (LMTK3) Domain: An In Silico Study},
Journal = {CURRENT PROTEOMICS},
Year = {2017},
Volume = {14},
Number = {2},
Pages = {157-164},
Abstract = {Background: Estrogen receptor-alpha positive breast cancer is the most
   common dreadful disease and leading cause of death among women. In
   majority of human breast cancer, the interactions between kinases and ER
   alpha are considered to be critical in signaling pathway. Many kinases
   are known to regulate ER alpha activity. Recently Lemur tyrosine
   kinase-3 was identified as predictive oncogenic ER alpha regulator with
   a vital role in endocrine resistance. The role of LMTK3 in ERa
   regulation can be known by studying the interactions between them.
   Objectives: To understand the transient interactions between ER alpha
   and LMTK3 using computational technique.
   Method: ER alpha-LMTK3 complex structure was obtained using PatchDock
   server. The interacting residues and interface area between ERa and
   LMTK3 were identified using PDBsum. Molecular dynamics simulation was
   used to study the conformational dynamics of ER alpha-LMTK3 complex.
   Result: The approximate interface area of ER alpha-LMTK3 was found to be
   3175 angstrom(2) with atomic contact energy of 191.77 kcal/mol. PDBsum
   results revealed that some of the residues in C-terminal region of LMTK3
   displayed non-bonding interactions with the residues in the
   phosphorylation sites (Ser104 and Ser106) of ER alpha. We noticed the
   total number of interface residues in ER alpha-LMTK3 complex to be 50
   and the interface area for ER alpha as well as LMTK3 chain involved in
   interaction to be more than 2380 a2. From conformational dynamics study,
   ER alpha-LMTK3 complex structure was found to be stable.
   Conclusion: The outcomes of the current study enhance the understanding
   of interactions between ER alpha and LMTK3 which are thought to be
   critical in signaling pathway in majority of human breast cancers.},
