@article{ WOS:000327074200005,
Author = {Baden, Andrea L. and Wright, Patricia C. and Louis, Jr., Edward E. and
   Bradley, Brenda J.},
Title = {Communal nesting, kinship, and maternal success in a social primate},
Journal = {BEHAVIORAL ECOLOGY AND SOCIOBIOLOGY},
Year = {2013},
Volume = {67},
Number = {12},
Pages = {1939-1950},
Month = {DEC},
Abstract = {Communal nesting, where several mothers regularly pool and cooperatively
   rear offspring, is unusual in mammals. This type of crSching behavior is
   especially rare among primates, with the notable exceptions of humans,
   some nocturnal strepsirrhines, and-as we show in this
   study-black-and-white ruffed lemurs (Varecia variegata). Here, we
   combine data on nesting behavior, genetic relatedness, and infant
   survivorship to describe variation in ruffed lemur infant care and to
   examine the potential benefits of ruffed lemur communal breeding.
   Reproductive events were rare, and females produced litters
   (synchronously) only once in 6 years of observation. We show that not
   all mothers participate in communal crSches, but those that did had
   greater maternal success; communal breeders spent more time feeding and
   their offspring were more likely to survive. Although cooperating
   mothers were often related, females also cooperated with non-kin, and
   those who shared infant care responsibilities had greater maternal
   success than mothers who did not participate. If there is indeed a
   causal link between maternal cooperation and reproductive success, this
   unusual behavior, like that of human communal rearing, may have evolved
   via some combination of kin selection and mutualism.},
