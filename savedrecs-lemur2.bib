@article{ WOS:000180282500036,
Author = {Rumpler, Y},
Title = {Chromosomal and molecular primatology},
Journal = {EVOLUTIONARY ANTHROPOLOGY},
Year = {2002},
Volume = {11},
Number = {1},
Pages = {145-149},
Note = {Centenary Congress of the Zurich-Anthropological-Institute-and-Museum,
   ZURICH, SWITZERLAND, OCT 10-12, 1999},
Organization = {Zurich Anthropol Inst \& Museum},
Abstract = {The development of molecular biology techniques, such as those based on
   highly repetitive DNA and on widely used sequencing of mitochondrial and
   nuclear DNA, as well as the recent incursion of molecular biology
   techniques into the field of comparative cytogenetics, e.g., painting
