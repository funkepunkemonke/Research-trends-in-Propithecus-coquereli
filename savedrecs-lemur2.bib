Language = {English},
Affiliation = {SCHILLING, A (Corresponding Author), MNHN,ECOL GEN LAB,CNRS,URA 1183,4 AVE PETIT CHATEAU,F-91800 BRUNOY,FRANCE.},
DOI = {10.1095/biolreprod49.1.58},
