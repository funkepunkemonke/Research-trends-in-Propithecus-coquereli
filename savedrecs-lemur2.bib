Address = {TIERGARTENSTRASSE 17, D-69121 HEIDELBERG, GERMANY},
Type = {Article},
Language = {English},
Affiliation = {Fietz, J (Corresponding Author), Univ Marburg, Dept Anim Physiol, D-35032 Marburg, Germany.
