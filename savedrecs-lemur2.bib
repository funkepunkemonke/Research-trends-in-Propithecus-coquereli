   SW Texas State Univ, Dept Biol, San Marcos, TX 78666 USA.
   Univ Zurich, Inst Anthropol, Zurich, Switzerland.},
DOI = {10.1002/1098-2345(200101)53:1<1::AID-AJP1>3.0.CO;2-J},
ISSN = {0275-2565},
Keywords = {primates; Indridae; Propithecus; molecular phylogeny; sequencing;
   mitochondrial DNA},
Keywords-Plus = {MITOCHONDRIAL-DNA; MADAGASCAR; PRIMATES; POSITION; HYPOTHESIS;
