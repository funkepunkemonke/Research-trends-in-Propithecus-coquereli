@article{ WOS:000692272200015,
Author = {Tabuce, Rodolphe and Lihoreau, Fabrice and Mees, Florias and Orliac,
   Maeva J. and De Putter, Thierry and Smith, Thierry},
Title = {A reassessment of the Oligocene hyracoid mammals from Malembo, Cabinda,
   Angola},
