@article{ WOS:A1991EU88000006,
Author = {WOODALL, PF},
Title = {AN ULTRASTRUCTURAL-STUDY OF THE SPERMATOZOA OF ELEPHANT SHREWS
   (MAMMALIA, MACROSCELIDEA) AND ITS PHYLOGENETIC IMPLICATIONS},
Journal = {JOURNAL OF SUBMICROSCOPIC CYTOLOGY AND PATHOLOGY},
Year = {1991},
Volume = {23},
Number = {1},
Pages = {47-58},
Month = {JAN},
Abstract = {The fine structure of spermatozoa from the cauda epididymidis of six
   species of elephant shrews (Petrodromus tetradactylus, Macroscelides
   proboscideus, Elephantulus brachyrhynchus, E. edwardii, E. intufi, E.
   myurus) is described.  The spermatozoa of all species are generally
   similar with minor interspecific differences.  The head is long and
   flattened, capped with an acrosome which has a long apical segment and
   which extends distally for 60-70\% of the length of the nucleus.  A
   prominent subacrosome is present in all species, in Elephantulus spp. it
   is anvil-shaped with two lateral `barbs'.  Axonemal coarse fibres 1, 5,
   6 and 9 are larger than the others.  In E. edwardii the coarse fibres
   are particularly large and are associated with a wider and longer
   midpiece and more mitochondrial gyres than in any other species.  The
   ultrastructure of elephant shrew spermatozoa gives no support for a
   suggested relationship with lagomorphs, but shows some similarities with
   Megachiroptera, moles (Insectivora) and lemurs (Primates):  orders
   sometimes associated in the Archonta.},
