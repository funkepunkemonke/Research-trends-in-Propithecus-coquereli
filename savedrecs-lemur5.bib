@article{ WOS:000476557500005,
Author = {Glawischnig, W. and Auer, H. and Bago, Z. and Schoepf, K.},
Title = {Case report: Echinococcus multilocularis infection of a Japanese Macaque
   (Macaca fuscata)},
Journal = {WIENER TIERARZTLICHE MONATSSCHRIFT},
Year = {2019},
Volume = {106},
Number = {7-8},
Pages = {161-167},
Abstract = {Alveolar echinococcosis is caused by the larval stage of the tapeworm
   Echinococcus multilocularis and is considered one of the most pathogenic
   helminthic zoonoses. In central Europe, various rodents such as voles
   and muskrats act as intermediate hosts whereas humans act as aberrant
   intermediate hosts. We report the case of a naturally acquired alveolar
   echinococcosis in May 2018 that was diagnosed in a female Japanese
   macaque (Macaca fuscata) from a small zoo in the Tyrolean mountains at
   an altitude of 1100 metres above sea level. The 21-year-old female
   monkey started showing clinical signs such as a slowly progressing
   enlargement of the belly two years before death. Pathological findings
   focused on the liver, which was severely enlarged and showed two
   tumour-like tissue masses with coalescing hydatid cysts of 1 cm in
   diameter. The tumour-like enlargements were connected by anatomically
   unchanged liver tissue. The cutting surface of the altered tissue was
   homogenous, yellowish and firm. Histology revealed a severe
   granulomatous hepatitis with the presence of cysts including
   protoscolices. Larval infection with Echinococcus multilocularis was
   confirmed by histopathology and PCR. Echinococcus multilocularis is
   common in foxes in the Tyrolean mountains and recent work has revealed a
   prevalence of about 33 \% in the fox population. The Japanese monkey was
   kept in a fenced enclosure for several years and was suspected to have
   contracted the infection by contact with fox faeces (e. g. via feed or
   zookeepers). This is the first report of alveolar echinococcosis in a
   captive monkey in Austria.},
