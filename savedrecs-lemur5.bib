@inproceedings{ WOS:000248078503128,
Author = {Xu, Xuheng and Zhu, Weizhong and Zhang, Xiaodan and Hu, Xiaohua and
   Song, Il-Yeol},
Book-Group-Author = {IEEE},
Title = {A comparison of local analysis, global analysis and ontology-based query
   expansion strategies for bio-medical literature search},
Booktitle = {2006 IEEE INTERNATIONAL CONFERENCE ON SYSTEMS, MAN, AND CYBERNETICS,
   VOLS 1-6, PROCEEDINGS},
Series = {IEEE International Conference on Systems Man and Cybernetics Conference
   Proceedings},
Year = {2006},
Pages = {3441+},
Note = {IEEE International Conference on Systems, Man and Cybernetics, Taipei,
   TAIWAN, OCT 08-11, 2006},
Organization = {IEEE Syst, Man \& Cybernet Soc; Minist Educ; Natl Sci Council; Natl
   Taipei Univ Technol; Natl Chiao Tung Univ},
Abstract = {In this paper we present the design and evaluation of our biomedical
   literature searching approaches using the TREC 2004 ad hoc retrieval
   task in the Genomics track. The main approach taken in our system is to
   expand queries by exploiting the three widely used strategies - local
   analysis, global analysis, and ontology-based term re-weighting across
   various search engines. The experimental results show that (4)
   ontology-based term re-weighting provides the best results among the
   three query expansion strategies, (2) expanding the initial query with
   more precise ontology-based term enhances LSI based local analysis
   substantially, and (3) including context to term re-weighting and LSI
   further improves the precision. Experimental results also show that the
   ontology-based term re-weighting with LUCENE or LEMUR search engines
   increases the average precision by up to 20.3\% or 12.1 \%,
   respectively, compared to that of the baseline runs. In addition, the
   LSI-based local analysis increases the average precision by 9.2\% with
   LEMUR search engine. We believe the approaches of the term re-weighting
   and LSI-based local analysis may be exploited in other bio-medical
   domains.},
