Affiliation = {OVERDORFF, DJ (Corresponding Author), DUKE UNIV, DEPT BIOL ANTHROPOL \& ANAT, 3705-B ERWIN RD, WHEELER BLDG, DURHAM, NC 27705 USA.},
DOI = {10.1002/ajp.1350280304},
