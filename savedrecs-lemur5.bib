@article{ WOS:000220461900017,
Author = {Ibanez, JC and Miranda, HC and Balaquit-Ibanez, G and Afan, DS and
   Kennedy, RS},
Title = {Notes on the breeding behavior of a Philippine Eagle pair at Mount
   Sinaka, Central Mindanao},
Journal = {WILSON BULLETIN},
Year = {2003},
Volume = {115},
Number = {3},
Pages = {333-336},
Month = {SEP},
Abstract = {We documented the breeding behavior and diet of a Philippine Eagle
   (Pithecophaga jefferyi) pair from July 1999 to January 2000 in an
   isolated forest in Central Mindanao. We observed eight distinct
   courtship displays and several activity patterns on the nest. Copulation
   started two months prior to egg laying and continued until the first
   month of incubation, with a mean of 1.5 copulations per day. Seventyfour
   percent of the time devoted to incubation was by the female. The
   incubation period lasted 58 days. Throughout the incubation and early
   brooding phases the male provided food for the female and the young.
   Diet consisted of 17 prey items of four vertebrate taxa, mostly mammals,
   with civet cats (Family Viverridae) and flying lemur (Cynocephatus
   volans) representing the bulk of the diet.},
