Address = {530 WALNUT STREET, STE 850, PHILADELPHIA, PA 19106 USA},
Type = {Article},
Language = {English},
Affiliation = {Barrow, EC (Corresponding Author), Univ Oxford, Dept Earth Sci, S Parks Rd, Oxford OX1 3AN, England.
   Barrow, Eugenie C., Univ Oxford, Dept Earth Sci, Oxford OX1 3AN, England.
   Seiffert, Erik R., SUNY Stony Brook, Dept Anat Sci, Hlth Sci Ctr, Stony Brook, NY 11794 USA.
   Simons, Elwyn L., Duke Lemur Ctr, Div Fossil Primates, Durham, NC 27705 USA.},
