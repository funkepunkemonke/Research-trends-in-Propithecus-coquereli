@article{ WOS:000229390700020,
Author = {Pochron, ST and Wright, PC},
Title = {Dance of the sexes},
Journal = {NATURAL HISTORY},
Year = {2005},
Volume = {114},
Number = {5},
Pages = {34-39},
Month = {JUN},
Abstract = {The lemur species, Propithecus edwardsi or Milne-Edwards sifaka, needs
   some unusual traits to survive on the island of Madagascar. Males and
   females do not often come into conflict but when they do it seems the
   males allow the femalesto win. The conflicts usually arise over food and
   often happen around the mating season. The reason as to why the males
   allow the females to win extra calories is unclear.},
