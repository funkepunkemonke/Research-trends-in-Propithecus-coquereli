@article{ WOS:000461240000005,
Author = {Pifferi, Fabien and Terrien, Jeremy and Perret, Martine and Epelbaum,
   Jacques and Blanc, Stephane and Picq, Jean-Luc and Dhenain, Marc and
   Aujard, Fabienne},
Title = {Promoting healthspan and lifespan with caloric restriction in primates},
Journal = {COMMUNICATIONS BIOLOGY},
Year = {2019},
Volume = {2},
Month = {MAR 15},
Abstract = {Recent data confirmed the efficiency of caloric restriction for
   promoting both healthspan and lifespan in primates, but also revealed
   potential adverse effects at the central level. This paper proposes
   perspectives and future directions to counterbalance potential adverse
   effects. Efforts should be made in combining nutrition-based clinical
   protocols with therapeutic and/or behavioral interventions to aim for
   synergetic effects, and therefore delay the onset of age-related
   diseases without adverse effects.},
Publisher = {NATURE PORTFOLIO},
Address = {HEIDELBERGER PLATZ 3, BERLIN, 14197, GERMANY},
