@inproceedings{ WOS:000082419500022,
Author = {Uhl, NW and Dransfield, J},
Editor = {Henderson, A and Borchsenius, F},
Title = {Genera Palmarum after ten years},
Booktitle = {EVOLUTION, VARIATION, AND CLASSIFICATION OF PALMS},
Series = {MEMOIRS OF THE NEW YORK BOTANICAL GARDEN},
Year = {1999},
Volume = {83},
Pages = {245-253},
Note = {Meeting on Evolution, Variation, and Classification of Palms, NEW YORK
   BOTAN GARDEN, NEW YORK, NY, JUN 18-20, 1997},
Organization = {New York Botan Garden; Ctr Trop Biodivers; Aarhus Univ; Copenhagen Univ},
Abstract = {Research on palms has: burgeoned in the decade since Genera Palmarum
   (GP) appeared. In 1987, approximately 80 genera were in need of
   monographs, 45 of these are now complete or in progress, leaving about
   35 genera still lacking a modern assessment. Fifteen of the 200 numbered
   genera and Antongilia (suggested in GP to be a species of Neodypsis)
   have been placed in synonymy. Thus, the number of genera has changed
   from 200 to 189. Four new genera, Satranala (Coryphoideae).
   Lemurophoenix and Voanioala (Arecoideae), and Aphandra
   (Phytelephantoideae) have been discovered, described, and named, and a
   fifth new genus, Paschalococos, based on a recently extinct cocoid palm
   from Easter Island, has also been described and named. The little known
   Medemia, Masoala, Carpoxylon, and Goniocladus and several other
   incompletely known genera have been rediscovered and reassessed. Major
   progress has been made in morphology and anatomy. including a synthesis
   of the structural biology of palms, and extensive contributions to stem
   anatomy in subfamily Calamoideae. Root anatomy has been studied for all
   subfamilies except Arecoideae. Pollen, with an assessment of fossil
   pollens and their assignment to palms, and chromosomal karyotype
   structure have been surveyed for all subfamilies. Cladistic analyses of
   morphological and of molecular characters from both restriction fragment
   length polymorphisms in chloroplast DNA and sequencing of chloroplast
   and nuclear genes have provided insights as to what groups are
   monophyletic and how some are related. However, if our goal is a
   phylogeny of all genera and a revision of the classification, such
   studies are far from complete: indeed, many are being reported for the
   first time in this symposium.},
