@article{ WOS:000310339100038,
Author = {Garcia, Enrique and Urena, Jesus and Jesus Garcia, Juan and Carmen
   Perez, Ma and Ruiz, Daniel},
Title = {Efficient Generator/Correlator of GPC Sequences for QS-CDMA},
Journal = {IEEE COMMUNICATIONS LETTERS},
Year = {2012},
Volume = {16},
Number = {10},
Pages = {1676-1679},
Month = {OCT},
Abstract = {This paper presents a novel efficient generator and correlator for
   Generalized Pairwise Complementary (GPC) sequences. The novel
   generator/correlator is constructed from an efficient
   generator/correlator for two uncorrelated Golay pairs of length 2(N) .
   10(M) . 26(P). The proposed architecture permits to transmit both pairs
   in a quadrature modulation scheme (QAM or QPSK). The reduced number of
   operations needed in comparison with a straightforward correlator and
   the large Zero Correlation Zone (ZCZ) makes these sequences good
   candidates to be used in QS-CDMA (Quasi-Synchronous CDMA).},
