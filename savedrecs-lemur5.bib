@article{ WOS:000404211800014,
Author = {Bethge, Janina and Wist, Bianca and Stalenberg, Eleanor and Dausmann,
   Kathrin},
Title = {Seasonal adaptations in energy budgeting in the primate Lepilemur
   leucopus},
Journal = {JOURNAL OF COMPARATIVE PHYSIOLOGY B-BIOCHEMICAL SYSTEMS AND
   ENVIRONMENTAL PHYSIOLOGY},
