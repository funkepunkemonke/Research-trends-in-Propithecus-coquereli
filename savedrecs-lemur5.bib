@article{ WOS:000318039300014,
Author = {Morse, Paul E. and Daegling, David J. and McGraw, W. Scott and Pampush,
   James D.},
Title = {Dental Wear Among Cercopithecid Monkeys of the Tai Forest, Cote d'Ivoire},
Journal = {AMERICAN JOURNAL OF PHYSICAL ANTHROPOLOGY},
Year = {2013},
Volume = {150},
Number = {4},
Pages = {655-665},
Month = {APR},
Abstract = {Studies of dental macrowear can be useful for understanding masticatory
   and ingestive behavior, life history, and for inferring dietary
   information from the skeletal material of extinct and extant primates.
   Such studies to date have tended to focus on one or two teeth,
   potentially missing information that can be garnered through examination
   of wear patterns across the tooth row. Our study measured macrowear in
   the postcanine teeth of three sympatric cercopithecid species from the
   Tai Forest, Cote d'Ivoire (Cercocebus atys, Procolobus badius, and
   Colobus polykomos), whose diets have been well-described. Inter-specific
   analyses suggest that different diets and ingestive behaviors are
   characterized by different patterns of wear across the molar row, with
   Cercocebus atys emphasizing tooth use near P-4-M-1, P. badius
   emphasizing a large amount of tooth use near M-2-M-3, and Colobus
   polykomos exhibiting wear more evenly across the postcanine teeth.
   Information regarding differential tooth use across the molar row may be
   more informative than macrowear analysis of isolated teeth for making
   inferences about primate feeding behavior. Am J Phys Anthropol
   150:655-665, 2013. (C) 2013 Wiley Periodicals, Inc.},
