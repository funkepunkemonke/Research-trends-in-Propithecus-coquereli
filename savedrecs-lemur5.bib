@article{ WOS:000488552700001,
Author = {Kuszkowski, F. S. and Lucioli, J. and De Souza Junior, J. Cesar and
   Hirano, Z.},
Title = {Post mortem evaluation of liver iron overload in howler monkeys
   (Alouatta clamitans)},
Journal = {REVUE DE MEDECINE VETERINAIRE},
Year = {2019},
Volume = {170},
Number = {7-9},
Pages = {118-122},
Abstract = {Hemosiderosis is an iron accumulation syndrome that can advance to
   hemochromatosis, which is the accumulation associated with disorders in
   organ function. Although howler monkeys have a specific iron metabolism
   related to their fur colour, iron accumulation in this specie has never
   been investigated. Hemochromatosis has been reported in other non-human
   primates. The main goal of the present study is to investigate post
   mortem iron accumulation on 105 livers of Allouata clamitans, from the
   biological material collection of the Centro de Pesquisas Biologicas de
   Indaial. This paper compares post mortem iron accumulation in the liver
   of free-ranging and captive Howler monkeys through Prussian Blue
   staining. For microscopic study, Hematoxilin-eosin stain exposed the
   cellular morphology and Prusian blue stain quantified the amount of iron
   accumulation. Chi-square test of independence and Prevalence Test were
   used for statistical evaluation. From 105 animals, 60 (57\%) were
