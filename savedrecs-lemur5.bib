@inproceedings{ WOS:000248944000005,
Author = {Brauner, Paul and Houtmann, Clement and Kirchner, Claude},
Title = {Principles of superdeduction},
Booktitle = {22ND ANNUAL IEEE SYMPOSIUM ON LOGIC IN COMPUTER SCIENCE, PROCEEDINGS},
