@article{ WOS:000345194100021,
Author = {Bober, Jonathan and Conrey, J. Brian and Farmer, David W. and Fujii,
   Akio and Koutsoliotas, Sally and Lemurell, Stefan and Rubinstein,
   Michael and Yoshida, Hiroyuki},
Title = {The highest lowest zero of general L-functions},
Journal = {JOURNAL OF NUMBER THEORY},
Year = {2015},
Volume = {147},
Pages = {364-373},
Month = {FEB},
Abstract = {Stephen D. Miller showed that, assuming the Generalized Riemann
   Hypothesis, every entire L-function of real archimedean type has a zero
   in the interval 1/2 + it with -t(0) < t < t(0), where to approximate to
   14.13 corresponds to the first zero of the Riemann zeta function. We
   give a numerical example of a self-dual degree-4 L-function whose first
   positive imaginary zero is at t(1) approximate to 14.496. In particular,
   Miller's result does not hold for general L-functions. We show that all
   L-functions satisfying some additional (conjecturally true) conditions
   have a zero in the interval (-t(2), t(2)) with t(2) approximate to
   22.661. (C) 2014 Elsevier Inc. All rights reserved.},
