@article{ WOS:000428304300186,
Author = {Georgious, Ramy and Garcia, Jorge and Garcia, Pablo and
   Navarro-Rodriguez, Angel},
Title = {A Comparison of Non-Isolated High-Gain Three-Port Converters for Hybrid
   Energy Storage Systems},
