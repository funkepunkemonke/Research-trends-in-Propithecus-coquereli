Journal = {INTERNATIONAL JOURNAL OF PUBLIC ADMINISTRATION IN THE DIGITAL AGE},
Year = {2016},
Volume = {3},
Number = {1, SI},
Pages = {43-52},
Month = {JAN-MAR},
Abstract = {Organizational citizenship behavior (OCB) and perceived organizational
   support (POS) have been explored extensively in the literature. Davis
   (2013) highlights the need for more Information Systems (IS) research in
   this area. In this study the authors explore how technological
