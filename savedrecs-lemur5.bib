@article{ WOS:000328707400005,
Author = {Heymann, Eckhard W. and Zinner, Dietmar and Ganzhorn, Joerg U.},
Title = {Disproportional Representation of Primates in the Ecological Literature},
Journal = {PLOS ONE},
Year = {2013},
Volume = {8},
Number = {12},
Month = {DEC 10},
Abstract = {We address the question why papers dealing with the ecology of primates
   are so sparsely represented in the general ecological literature. A
   literature analyses based on entries in Web of Science and PrimateLit
   reveals that despite a large number of papers published on primates in
   general and on the ecology of primates, only a very small fraction of
   these papers is published in high-ranking international ecological
   journals. We discuss a number of potential reasons for the disproportion
   and highlight the problems associated with experimental research on wild
   primates and constraints on sample size as major issues.},
