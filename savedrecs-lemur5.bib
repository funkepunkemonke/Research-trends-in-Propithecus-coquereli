   Selig, Keegan R.; Silcox, Mary T., Univ Toronto Scarborough, Dept Anthropol, Toronto, ON, Canada.
   Schroeder, Lauren, Univ Toronto Mississauga, Dept Anthropol, Mississauga, ON, Canada.
   Schroeder, Lauren, Univ Cape Town, Human Evolut Res Inst, Cape Town, Western Cape, South Africa.},
DOI = {10.1080/02724634.2021.1995738},
EarlyAccessDate = {JUL 2021},
