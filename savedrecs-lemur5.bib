@article{ WOS:000326428600017,
Author = {Wamba, Samuel Fosso and Anand, Abhijith and Carter, Lemuria},
Title = {A literature review of RFID-enabled healthcare applications and issues},
Journal = {INTERNATIONAL JOURNAL OF INFORMATION MANAGEMENT},
Year = {2013},
Volume = {33},
Number = {5},
Pages = {875-891},
Month = {OCT},
Abstract = {Despite the rising implementation of RFID technology-based healthcare
   services, few empirical studies have been conducted to assess the
   potential of this technology within the healthcare sector. The purpose
   of this study is to help initiate and direct such empirical research by
