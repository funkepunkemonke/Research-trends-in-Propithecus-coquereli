@article{ WOS:000734887000002,
Author = {Zeng, Y. and Pu, Y. and Niu, L. L. and Deng, J. B. and Zeng, D. and
   Amato, K. R. and Li, Y. and Zhou, Y. and Lin, Y. C. and Wang, J. and Wu,
