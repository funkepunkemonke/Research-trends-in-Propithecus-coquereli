@article{ WOS:000175467200009,
Author = {Palagi, E and Gregorace, A and Tarli, SMB},
Title = {Development of olfactory behavior in captive ring-tailed lemurs (Lemur
   catta)},
Journal = {INTERNATIONAL JOURNAL OF PRIMATOLOGY},
Year = {2002},
Volume = {23},
Number = {3},
Pages = {587-599},
Month = {JUN},
Abstract = {We conducted a long-terra research project (1996-1999) on developmental
   aspects of olfactory behavior in ring-tailed lemurs to document the
   ontogenetic sequence of olfactory behavior; including the possible
   presence of sexual differences, and the maturation of scent-marking. The
   subjects were a group of 18 lemurs housed in the Pistoia Zoo (Tuscany,
   Italy), which we observed during 1,735 h via all-occurrences sampling
   scan-animals sampling and ad libitum sampling methods. We determined the
   time sequence of olfactory exploration and of scent-marking patterns,
   and investigated sexual differences in timing and frequency. We also
   followed the development of scent-marking through the juvenile and
   adolescent phases taking into account the two aspects of motor control
   and of the acquisition of social competence. On the whole, we found that
   olfactory investigation appears and,,natures earlier than scent-marking.
   Moreover, olfactory investigation of conspecifics appeared later than
   substrate exploration, and seemed to follow a sequence of increasing
   level of acceptance by the receiver. Social play is very important for
   the maturation of the gestural component of scent-marking. The olfactory
   behavioral pattern appeared to nature during the juvenile and adolescent
   phases. Although sexual maturation had obvious influence on the
   development of olfactory behavior, the onset of scent-marking patterns
   was only partially parallel to sexual maturation.},
