@article{ WOS:000074054600001,
Author = {Gachot-Neveu, H},
Title = {RAPDs: Demonstration of a useful tool for the analysis and the control
   of breeding management. Application to the study of lemur breeding.},
Journal = {SCIENCES ET TECHNIQUES DE L ANIMAL DE LABORATOIRE},
Year = {1998},
Volume = {23},
Number = {1},
Pages = {7-18},
Note = {24th Colloquium of the SFEA, LA GRANDE MOTTE, FRANCE, JUN 16-18, 1997},
Organization = {SFEA},
Abstract = {First perfected on humans, and then on animals, techniques of genetic
   analysis from DNA have been great improved over the last twenty years.
   Unlike proteins, DNA is present and directly accessible in most of cells
   : it is stable and sufficiently variable to constitute a very good
   individual genetic marker. Compared with the classical analysis of
   restriction fragments (RFLP), DNA amplification currently allows a rapid
   genetic analysis from very small amounts of DNA. The random amplication
   of DNA (Random Amplified Polymorphic DNA : RAPD) produces series of DNA
   fragments which are anonymous and polymorphic, thus representing good
   individual genetic markers.
   Applied to the study of the genetic structure of different lemur
   colonies, the RAPD method revealed some advantages and disadvantages of
   a universal but random amplification of the genome. We conducted various
   studies on an individual scale and then at a group level, in two species
   of lemurs (Microcebus murinus and Eulemur macaco macaco). We used RAPDs
   as a tool for the genetic control of captive breeding management,
   through qualitative and quantitative measurement of the genetic
   diversity of a group. With these markers, we can now monitor variations
   in the genetic diversity according to different parameters in
   association with the management of captive breeding.},
