@article{ WOS:000075795700005,
Author = {Clark, M},
Title = {The mongoose lemur Eulemur mongoz on Anjouan, Comores},
Journal = {DODO-JOURNAL OF THE WILDLIFE PRESERVATION TRUSTS},
Year = {1997},
Volume = {33},
Pages = {36-44},
Abstract = {The mongoose lemur Eulemur mongoz is a prosimian primate native to
   Madagascar. Populations also occur on the islands of Anjouan and Moheli
   in the Federal Islamic Republic of the Comores. The current status of
   the lemur is unknown, but thought to be declining due to habitat
   destruction. The author carried out a short survey by interviewing
   people in 22 villages on Anjouan, in order to gauge the attitude of
   local people to the presence of these animals. The results of the survey
   strongly indicated that mongoose lemurs are routinely caught and kept as
   pets by local children. However, there was no compelling evidence for
   other forms of persecution.},
