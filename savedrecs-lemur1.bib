@article{ WOS:000364828500001,
Author = {Bochkov, Andre V. and Klompen, Hans and Junge, Randall E. and Williams,
   Cathy V.},
Title = {Lemuralges propithecus sp n. (Acariformes: Psoroptidae), an ectoparasite
   of the diademed sifaka Propithecus diadema (Primates: Indriidae)},
Journal = {FOLIA PARASITOLOGICA},
Year = {2015},
Volume = {62},
Month = {JAN 28},
Abstract = {A new species of the genus Lemuralges Fain, 1963 (Acariformes:
   Psoroptidae: Makialginae) is described from the Malagasy lemur
   Propithecus diadema (Bennett) (Primates: Indriidae) based on all
   postembryonic instars. This new species differs from the only known
   species in this genus, Lemuralges intermedius Fain, 1963, by the
   following features: both sexes of L. propithecus sp. n. show aypair of
   medioventral projections of the subcapitulum (vs without projections in
   L. intermedius) and the propodonotal shield is slightly ornamented (vs
   unornamented); in males the hysteronotal shield is completely covered by
   longitudinal striae (vs median part without striae), setae c2 are
   120-140 mu m long (vs 200-210 mu m long), and femur III has a short
   transverse furrow dorsally (vs a longitudinal furrow); in females, setae
   h2 are, at least, 2 times shorter than h3 (vs slightly longer, or
   subequal to, h3), tibia IV has a ventro-apical projection (vs without
   projection). Larvae and protonymphs of the new species show some unique
   developmental delays. Female and male tritonymphs differ by their
   external morphology.},
