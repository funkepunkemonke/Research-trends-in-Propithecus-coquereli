@article{ WOS:000257220100008,
Author = {Raharison, F. and Mogicato, G. and Terefe, G. and Sautet, J.},
Title = {Normal echocardiography of brown lemur (Eulemur fulvus)},
Journal = {REVUE DE MEDECINE VETERINAIRE},
Year = {2008},
Volume = {159},
Number = {5},
Pages = {312-319},
Month = {MAY},
Abstract = {Cardiac ultrasound is a noninvasive technique that is commonly used to
   evaluate cardiac structure and function. There are no standard
   echocardiographic datas for the brown lemur (Eulemur fulvus), which is
   the most common and widespread type of this species on the island of
   Madagascar. These reference range values would nevertheless be useful in
   zoos, where echography is to become a top-grade diagnostic tool, for
   being non invasive. This study concerned 16 brown lemurs. After
   sedation, echographic examinations in two dimensionnal mode, M-Mode and
   Doppler mode were performed on the animals : The orientation of the
   heart is intermediate between that of animals possessing a clavicle and
   that of the animals which lack it. M-Mode, the fractional shortening
   (mean AD, per cent), index of left ventricular performance, is 54.63 per
   cent (standard deviation: 14.68 per cent). In Doppler mode, the mean
   aortic velocity is 0,99 m per s (standard deviation: 0,21 m per s) and
   the mean pulmonic flow velocity is 0,95 m per s (standard deviation:
   0,21 m per s). These echocardiographic values can serve as reference
   range values for further studies concerning the cardiac function in the
   brown lemur.},
