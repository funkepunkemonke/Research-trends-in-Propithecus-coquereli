@article{ WOS:000640536000001,
Author = {Achorn, Angela and Winking, Jeffrey},
Title = {A comparison of intestinal parasite infections and activity budgets in
   three free-ranging vs. enclosure-housed lemur species (Lemur catta,
   Varecia rubra, and Eulemur mongoz)},
Journal = {JOURNAL OF MEDICAL PRIMATOLOGY},
Year = {2021},
Volume = {50},
Number = {3},
Pages = {189-192},
Month = {JUN},
Abstract = {This study examines how range size affects intestinal parasite
   infections, and how such infections affect activity budgets in captive
   lemurs. There were no differences in parasite richness or intensity
   attributable to habitat type. However, there was a strong suggestion
   that parasite loads reduce activity levels.},
