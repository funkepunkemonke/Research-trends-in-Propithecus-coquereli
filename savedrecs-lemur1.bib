ISSN = {0164-0291},
EISSN = {1573-8604},
Keywords = {Cheirogaleids; Life histories; Madagascar},
Keywords-Plus = {RANOMAFANA NATIONAL-PARK; MADAGASCAR; HISTORY; REPRODUCTION; ECOLOGY;
