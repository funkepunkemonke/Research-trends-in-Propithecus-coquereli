@article{ WOS:000295317600032,
Author = {Juan-Salles, Carles and Mainez, Mireia and Marco, Alberto and Malabia
   Sanchis, Ana M.},
Title = {Localized toxoplasmosis in a ring-tailed lemur (Lemur catta) causing
   placentitis, stillbirths, and disseminated fetal infection},
Journal = {JOURNAL OF VETERINARY DIAGNOSTIC INVESTIGATION},
Year = {2011},
Volume = {23},
Number = {5},
Pages = {1041-1045},
Month = {SEP},
Abstract = {Localized, myocardial toxoplasmosis contributed to the death of a female
   ring-tailed lemur (Lemur catta) 1 week after the delivery of 4 stillborn
   offspring with disseminated toxoplasmosis; the diagnosis was obtained by
   histopathology and immunohistochemistry in all 5 lemurs. Varying degrees
   of placentitis and placental edema with intralesional Toxoplasma gondii
   immunolabeling were observed in the 3 available placentas. The dam had
   severe myocarditis, and T. gondii antigen was only detected in the
   myocardial lesions. Disseminated toxoplasmosis with mild encephalitis
   was noted in all 4 fetuses, and 2 of the fetuses had mild acute
   multifocal hepatic necrosis. Fetal death was attributed to placental
   insufficiency with subsequent hypoxia and amniotic fluid aspiration.},
