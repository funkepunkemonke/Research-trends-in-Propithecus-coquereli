@article{ WOS:000081017600003,
Author = {Beattie, J},
Title = {New natural exhibit for the Alaotran gentle lemur Hapalemur griseus
   alaotrensis at the Jersey Wildlife Preservation Trust},
Journal = {DODO-JOURNAL OF THE WILDLIFE PRESERVATION TRUSTS},
Year = {1998},
Volume = {34},
Pages = {18-24},
Abstract = {The design of a naturalistic enclosure for a breeding group of Alaotran
   gentle lemurs Hapalemur griseus alaotrensis at Jersey Wildlife
   Preservation Trust is described. The enclosure measures 800 m(2) and
