@article{ WOS:000691308000011,
Author = {Kosasih, Wawan and Rosmalina, R. Tina and Risdian, Chandra and Saepudin,
   Endang and Priatni, Sri},
Title = {Production of Omega-3 Fatty Acids by Enzymatic Hydrolysis from Lemuru
   Fish By-Products},
