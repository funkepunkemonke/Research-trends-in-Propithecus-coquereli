@article{ WOS:000506800100003,
Author = {Fulwood, Ethan L.},
Title = {Ecometric modelling of tooth shape and precipitation gradients among
   lemurs on Madagascar},
Journal = {BIOLOGICAL JOURNAL OF THE LINNEAN SOCIETY},
Year = {2020},
Volume = {129},
Number = {1},
Pages = {26-40},
Month = {JAN},
Abstract = {Ecometric modelling relates spatial environmental variables to
   phenotypic characters to better understand morphological adaptation and
   help reconstruct past environments. Here, the community means of the
   dental topography metrics Dirichlet normal energy (DNE) and orientation
   patch count (OPC) are tested against annual precipitation and
   precipitation seasonality among lemurs across Madagascar. Dry, seasonal
   environments are expected to be associated with high DNE and OPC, as
   lemurs living in these environments are more likely to rely on tougher
   foods. Ecometric models are also used to calculate ecometric loads for
   lemur taxa hypothesized to be experiencing evolutionary disequilibria
   and to reconstruct annual precipitation and precipitation seasonality at
   the similar to 500 years BP subfossil cave site of Ankilitelo. DNE was
   highest in highly seasonal but wet environments. Seasonal exploitation
   of fallback foods and the availability of new leaves during wet periods
   may be most important in driving community DNE. OPC was weakly predicted
   by annual precipitation and seasonality but its distribution appeared to
   be driven by a stepwise increase in its community values in rainforest
   environments. The lemur fauna from Ankilitelo appears to resemble
   communities from moister environments than occur in the spiny desert
   zone in which the site is situated today.},
