@article{ WOS:000171348500022,
Author = {Kumamoto, AT and Houck, ML},
Title = {Cytogenetic identification of a hybrid owl monkey, Aotus nancymaae x
   Aotus lemurinus griseimembra},
Journal = {JOURNAL OF ZOO AND WILDLIFE MEDICINE},
