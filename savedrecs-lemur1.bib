@article{ WOS:000738762100003,
Author = {Itoigawa, Akihiro and Fierro, Fabrizio and Chaney, Morgan E. and
   Lauterbur, M. Elise and Hayakawa, Takashi and Tosi, Anthony J. and Niv,
   Masha Y. and Imai, Hiroo},
Title = {Lowered sensitivity of bitter taste receptors to beta-glucosides in
   bamboo lemurs: an instance of parallel and adaptive functional decline
   in TAS2R16?},
Journal = {PROCEEDINGS OF THE ROYAL SOCIETY B-BIOLOGICAL SCIENCES},
Year = {2021},
Volume = {288},
Number = {1948},
Month = {APR 14},
Abstract = {Bitter taste facilitates the detection of potentially harmful substances
   and is perceived via bitter taste receptors (TAS2Rs) expressed on the
   tongue and oral cavity in vertebrates. In primates, TAS2R16 specifically
   recognizes beta-glucosides, which are important in cyanogenic plants'
   use of cyanide as a feeding deterrent. In this study, we performed
   cell-based functional assays for investigating the sensitivity of
   TAS2R16 to beta-glucosides in three species of bamboo lemurs (Prolemur
   simus, Hapalemur aureus and H. griseus), which primarily consume
   high-cyanide bamboo. TAS2R16 receptors from bamboo lemurs had lower
   sensitivity to beta-glucosides, including cyanogenic glucosides, than
   that of the closely related ring-tailed lemur (Lemur catta). Ancestral
   reconstructions of TAS2R16 for the bamboo-lemur last common ancestor
   (LCA) and that of the Hapalemur LCA showed an intermediate sensitivity
   to beta-glucosides between that of the ring-tailed lemurs and bamboo
   lemurs. Mutagenetic analyses revealed that P. simus and H. griseus had
   separate species-specific substitutions that led to reduced sensitivity.
   These results indicate that low sensitivity to beta-glucosides at the
   cellular level-a potentially adaptive trait for feeding on cyanogenic
   bamboo-evolved independently after the Prolemur-Hapalemur split in each
   species.},
