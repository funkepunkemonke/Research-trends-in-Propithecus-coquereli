Title = {ENVIRONMENTAL AND SOCIAL DETERMINANTS OF SEXUAL FUNCTION IN THE MALE
   LESSER MOUSE LEMUR (MICROCEBUS-MURINUS)},
Journal = {FOLIA PRIMATOLOGICA},
Year = {1992},
