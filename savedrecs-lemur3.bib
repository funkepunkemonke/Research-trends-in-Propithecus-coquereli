   Rakotondravony, Romule, Univ Mahajanga, Fac Sci, Mahajanga, Madagascar.},
DOI = {10.1002/ajp.20732},
ISSN = {0275-2565},
EISSN = {1098-2345},
Keywords = {spatial niche partitioning; habitat selection; Madagascar; population
   density; altitude},
Keywords-Plus = {COMPETITIVE COEXISTENCE; SLEEPING SITES; POPULATION; NORTH;
   ENVIRONMENTS; DIVERSITY; COMMUNITY; ABUNDANCE; PRIMATES; ECOLOGY},
