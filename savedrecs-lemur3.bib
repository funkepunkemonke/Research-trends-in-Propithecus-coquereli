@article{ WOS:A1995QK62000011,
Author = {DELACOURTE, A and SAUTIERE, PE and WATTEZ, A and MOURTONGILLES, C and
   PETTER, A and BONS, N},
Title = {BIOCHEMICAL-CHARACTERIZATION OF TAU-PROTEINS DURING CEREBRAL AGING OF
   THE LEMURIAN PRIMATE MICROCEBUS-MURINUS},
Journal = {COMPTES RENDUS DE L ACADEMIE DES SCIENCES SERIE III-SCIENCES DE LA
   VIE-LIFE SCIENCES},
Year = {1995},
Volume = {318},
Number = {1},
Pages = {85-89},
Month = {JAN},
Abstract = {Tau proteins extracted from the brain of 12 adult microcebes ranging
   from 2 to 9 years old were characterized by Western blots, using
   immunological probes against normal and pathological human Tau proteins.
   In microcebes, the molecular weight of Tau proteins increases during
   aging, with variants of 52-54, 64, 67 kDa in the young adult and
   variants of 60 and 70 kDa in the oldest animal studied. The increase of
   the apparent molecular weight is due to a change of conformation and a
   stabilization in the `'hyperphosphorylated'' state, as revealed with
   phosphorylation-dependent monoclonal antibodies Tau-1 and AD2.
   Furthermore, AD1 specifically detected Alzheimer-type epitopes on the 60
   kDa Tau isoform om a very old microcebe. These results suggest that
   Microcebus murinus is an interesting model for the study of the
   biochemical dysfunctions that occur in the human brain during aging and
   Alzheimer disease.},
