@article{ WOS:A1997XF76600010,
Author = {Warren, RD},
Title = {Habitat use and support preference of two free-ranging saltatory lemurs
   (Lepilemur edwardsi and Avahi occidentalis)},
Journal = {JOURNAL OF ZOOLOGY},
Year = {1997},
Volume = {241},
Number = {2},
Pages = {325-341},
Month = {FEB},
Abstract = {Lepilemur edwardsi and Avahi occidentalis are two species of nocturnal,
   folivorous `vertical clingers and leapers' (VCL). They have a similar
   body mass and share the same morphological adaptation for leaping. In a
   field study under sympatric conditions at Ampijoroa, Madagascar,
   comparison of support use with support availability using Jacobs' D
   preference values (Jacobs, 1974) showed that both species actively chose
   or avoided branches with certain qualities. However, while both species
   showed a preference for small oblique and horizontal branches, they
   selected them at different heights in the forest and with varying
   degrees of preference and avoidance for the other available supports.
   Despite their traditional locomotor assignation, both species showed a
   surprisingly strong preference for horizontal supports. These striking
   variations in detail of support preference may aid the maintenance of
   species segregation and niche difference.},
