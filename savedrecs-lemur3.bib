@article{ WOS:A1992KF00600039,
Author = {VUILLAUMERANDRIAMANANTENA, M and GODFREY, LR and JUNGERS, WL and SIMONS,
   EL},
Title = {MORPHOLOGY, TAXONOMY AND DISTRIBUTION OF MEGALADAPIS - GIANT SUBFOSSIL
   LEMUR FROM MADAGASCAR},
Journal = {COMPTES RENDUS DE L ACADEMIE DES SCIENCES SERIE II},
Year = {1992},
Volume = {315},
Number = {13},
Pages = {1835-1842},
Month = {DEC 17},
Abstract = {The significance of anatomical differences among the three recognized
   species of Megaladapis is poorly understood, although functional aspects
   of megaladapid cranial and postcranial morphology have been widely
   discussed. M. edwardsi, the largest of the three species, is generally
   described as the prototypical megaladapid, and M. grandidieri and M.
   madagascariensis are often considered to be merely scaled-down versions
   of the former. We argue that this characterization fails to acknowledge
   consistent anatomical and functional differences between members of two
   clades, one comprising M. madagascariensis and M. grandidieri, and the
   other comprising only M. edwardsi. This Note establishes that dichotomy,
   examines new materials from the North in light of the dichotomy, and
   considers implications for niche differentiation within this genus.},
