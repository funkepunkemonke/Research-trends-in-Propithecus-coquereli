@article{ WOS:000635555700001,
Author = {Cruz, Daniel F. and Mitash, Nilay and Mu, Fangping and Farinha, Carlos
   M. and Swiatecka-Urban, Agnieszka},
Title = {Differential Gene Expression Analysis Reveals Global LMTK2 Regulatory
   Network and Its Role in TGF-beta 1 Signaling},
Journal = {FRONTIERS IN ONCOLOGY},
Year = {2021},
Volume = {11},
Month = {MAR 18},
Abstract = {Lemur tyrosine kinase 2 (LMTK2) is a transmembrane Ser/Thr kinase whose
   role has been increasingly recognized; however, when compared to other
   kinases, understanding of the LMTK2 networks and biological functions is
   still limited. Recent data have shown that transforming growth factor
   (TGF)-beta 1 plays a role in modulating LMTK2 function by controlling
   its endocytic trafficking in human bronchial epithelial cells. Here, we
   aimed to unveil the LMTK2 regulatory network and elucidate how it
   affects cellular functions and disease pathways in either TGF-beta 1
   dependent or independent manner. To understand how the LMTK2 and
   TGF-beta 1 pathways interconnect, we knocked down (KD) LMTK2 using
   small(si)RNA-mediated silencing in human bronchial epithelial CFBE41o-
   cells, treated cells with TGF-beta 1 or vehicle control, and performed
   differential gene expression analysis by RNA sequencing (RNAseq). In
   vehicle-treated cells, LMTK2 KD affected expression of 2,506 genes while
   it affected 4,162 genes after TGF-beta 1 stimulation. Bioinformatics
   analysis shows that LMTK2 is involved in diverse cellular functions and
   disease pathways, such as cell death and survival, cellular development,
   and cancer susceptibility. In summary, our study increases current
   knowledge about the LMTK2 network and its intersection with the TGF-beta
   1 signaling pathway. These findings will serve as basis for future
   exploration of the predicted LMTK2 interactions and signaling pathways.},
