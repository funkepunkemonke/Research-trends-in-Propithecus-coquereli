@article{ WOS:000452608600005,
Author = {Poindexter, Stephanie A. and Reinhardt, Kathleen D. and Nijman, Vincent
   and Nekaris, K. A. I.},
Title = {Slow lorises (Nycticebus spp.) display evidence of handedness in the
   wild and in captivity},
Journal = {LATERALITY},
Year = {2018},
Volume = {23},
Number = {6},
Pages = {705-721},
Month = {NOV 2},
Abstract = {It has been suggested that strepsirrhines (lemurs, lorises, and galagos)
   retain the more primitive left-hand preference, whilst monkeys and apes
   more regularly display a right-hand preference at the individual-level.
   We looked to address questions of laterality in the slow loris
   (Nycticebus spp.) using spontaneous observations of 7 wild individuals,
   unimanual tests in 6 captive individuals, and photos of 42 individuals
   in a bilateral posture assessing handedness at the individual- and
   group-level. During the unimanual reach task, we found at the
   individual-level, only 4 slow lorises showed a hand use bias (R: 3, L:
   1), Handedness index (HI) ranged from -0.57 to 1.00. In the wild
   unimanual grasp task, we found at the individual-level two individual
   showed a right-hand bias, the HI ranged from -0.19 to 0.70. The
   bilateral venom pose showed a trend toward a right-hand dominant grip in
   those photographed in captivity, but an ambiguous difference in wild
   individuals. There are many environmental constraints in captivity that
   wild animals do not face, thus data collected in wild settings are more
   representative of their natural state. The presence of right-handedness
   in these species suggests that there is a need to re-evaluate the
   evolution of handedness in primates.},
