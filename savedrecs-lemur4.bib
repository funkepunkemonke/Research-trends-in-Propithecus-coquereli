Type = {Review},
Language = {English},
Affiliation = {Dausmann, KH (Corresponding Author), Univ Hamburg, Inst Zool, Funct Ecol, Hamburg, Germany.
   Dausmann, Kathrin H.; Warnecke, Lisa, Univ Hamburg, Inst Zool, Funct Ecol, Hamburg, Germany.},
DOI = {10.1152/physiol.00050.2015},
ISSN = {1548-9213},
EISSN = {1548-9221},
