@article{ WOS:000327381400004,
Author = {Zhao, Guoqing and Guo, Jun and Li, Dong and Jia, Chengyou and Yin,
   Wanzhong and Sun, Ran and Lv, Zhongwei and Cong, Xianling},
Title = {MicroRNA-34a Suppresses Cell Proliferation by Targeting LMTK3 in Human
   Breast Cancer MCF-7 Cell Line},
Journal = {DNA AND CELL BIOLOGY},
Year = {2013},
Volume = {32},
Number = {12},
Pages = {699-707},
Month = {DEC 1},
Abstract = {Breast cancer remains the leading cause of cancer mortality in females,
   and about 70\% of the primary breast cancer patients are diagnosed
   ER-positive, which is the most common type of breast cancer.
   MicroRNA-34a (miR-34a) has been shown to be a master regulator of tumor
   suppression in many types of cancers including breast cancer. However,
   the role of miR-34a in ER-positive breast cancer has not been
   elucidated. Here, we find that in MCF-7, which is an ER-positive breast
   cancer cell line, miR-34a is remarkably downregulated after E2
   treatment. Overexpression of miR-34a by lentivirus suppresses cell
   proliferation, S phase ratio, and tumor formation in an E2-dependent
   manner in vitro. According to the mRNA sequence, lemur tyrosine kinase 3
   (LMTK3), which is an important regulator of estrogen receptor alpha
   (ER), is a predicted target of miR-34a. This is confirmed by dual
   luciferase reporter assay and the decrease of LMTK3 mRNA and protein
   levels after overexpression of miR-34a. Moreover, miR-34a overexpression
   decreases AKT signaling pathway and increases ER phosphorylation status.
   Taken together, these results suggest that miR-34a inhibits breast
   cancer proliferation by targeting LMTK3 and might be used as an anti-ER
   agent in breast cancer therapy.},
Publisher = {MARY ANN LIEBERT, INC},
Address = {140 HUGUENOT STREET, 3RD FL, NEW ROCHELLE, NY 10801 USA},
Type = {Article},
