@article{ WOS:000218864100031,
Author = {Baruah, Prabahita and Choudhury, Pradipta Ray and Talukdar, K. L.},
Title = {A STUDY OF SUPRACONDYLAR PROCESS OF HUMERUS},
Journal = {JOURNAL OF EVOLUTION OF MEDICAL AND DENTAL SCIENCES-JEMDS},
Year = {2012},
Volume = {1},
Number = {5},
Pages = {817-822},
Month = {NOV},
Abstract = {BACKGROUND: Supracondylar process, in human, is a rare, anomalous,
   beak-like bony process on the anteromedial surface of the humerus. It
   represents the embryologic vestigial remnant of climbing animals and
   seen in many reptiles, most marsupials, cats, lemurs and American
   monkeys. Aim is to study the supracondylar process of humerus. MATERIALS
   AND METHODS: 80 adult dry humeri were collected from Anatomy Department,
   Gauhati Medical College and were examined. RESULTS: Out of 80 humeri, we
   found one humerus of left side with a bony projection from anteromedial
   surface of its distal shaft. The bone was then examined, studied,
   photographed and its dimensions were recorded. CONCLUSION: Knowledge of
   this variation may be of great importance to anatomists and
   anthropologists, because of possible link to the origins and relations
   of the human races.},
