@article{ WOS:000345612700015,
Author = {Cadamuro, Valerie Choesmel and Bouakaze, Caroline and Croze, Myriam and
   Schiavinato, Stephanie and Tonasso, Laure and Gerard, Patrice and
   Fausser, Jean-Luc and Gibert, Morgane and Dugoujon, Jean-Michel and
   Braga, Jose and Balaresque, Patricia},
Title = {Determined about sex: Sex-testing in 45 primate species using a 2Y/1X
   sex-typing assay},
Journal = {FORENSIC SCIENCE INTERNATIONAL-GENETICS},
Year = {2015},
Volume = {14},
Pages = {96-107},
Abstract = {Sex-testing using molecular genetic technique is routinely used in the
   fields of forensics, population genetics and conservation biology.
   However, none of the assay used so far allows a non-ambiguous and
   successful sex determination for human and non-human primate species.
   The most widely used method, AMELY/X, and its alternatives suffer from a
   set of drawbacks in humans and can rarely be used in New World primate
   species. Here, we designed a new sex-typing assay using a multiplexed
   PCR amplification of UTX and UTY-homologous loci and combined
   male-specific SRY locus. This method was successfully tested on 1048
   samples, including 82 non-human primates from 45 Anthropoidea and
   Lemuriformes species and 966 human samples from 24 populations
   (Africans, Europeans, and South Americans). This sex-typing method is
   applicable across all primate species tested from Hominoidea to
   Indriidae, and also on various populations with different background
   origins; it represents a robust and cheap sex-typing assay to be used
   both by the anthropologist and primatologist communities. (C) 2014
   Elsevier Ireland Ltd. All rights reserved.},
