@inproceedings{ WOS:000238083200011,
Author = {Vinay, Vishwa and Cox, Ingemar J. and Milic-Frayling, Natasa and Wood,
   Ken},
Editor = {Lalmas, M and MacFarlane, A and Ruger, S and Tombros, A and Tsikrika, T and Yavlinsky, A},
Title = {Measuring the complexity of a collection of documents},
Booktitle = {ADVANCES IN INFORMATION RETRIEVAL},
Series = {Lecture Notes in Computer Science},
Year = {2006},
Volume = {3936},
Pages = {107-118},
Note = {28th European Conference on Information Retrieval (ECIR 2006), Imperial
   Coll London, S Kensington, London, ENGLAND, APR 10-12, 2005-2006},
Organization = {Queen Mary Univ London; City Univ; Engn \& Phys Sci Res Council; CEPIS;
   Google; GCHQ; Microsoft Res; Yahoo Res; Sharp; Apriorie; Lemur
   Consulting; MMKM; Elsevier},
Abstract = {Some text collections are more difficult to search or more complex to
   organize into topics than others. What properties of the data
   characterize this complexity? We use a variation of the Cox-Lewis
   statistic to measure the natural tendency of a set of points to fall
   into clusters. We compute this quantity for document collections that
   are represented as a set of term vectors. We consider applications of
   the Cox-Lewis statistic in three scenarios: comparing clusterability of
   different text collections using the same representation, comparing
   different representations of the same text collection, and predicting
   the query performance based on the clusterability of the query results
   set. Our experimental results show a correlation between the observed
   effectiveness and this statistic, thereby demonstrating the utility of
   such data analysis in text retrieval.},
