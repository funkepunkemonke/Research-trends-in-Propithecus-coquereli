@article{ WOS:A1995VG09700001,
Author = {Terranova, CJ},
Title = {Leaping behaviors and the functional morphology of strepsirhine primate
   long bones},
Journal = {FOLIA PRIMATOLOGICA},
Year = {1995},
Volume = {65},
Number = {4},
Pages = {181-201},
Abstract = {Recent examinations of the functional morphology underlying strepsirhine
   primate leaping behaviors have identified several cross-sectional
   features of long bones that can be associated with interspecific
   differences in leaping frequency and/or body size. In this study the
   cross-sectional parameters of strepsirhine fore- and hindlimb long bones
   are examined relative to body weight, bone length, phylogenetic
   affiliation, leaping frequency and landing kinematics. Humeral
   cross-sectional morphology is conservative relative to both phylogenetic
   and functional groupings. As a result, humeral cross-sectional area
   should prove useful as a size surrogate in future comparative studies.
   After the effects of bone length and body weight are controlled,
   `frequent' leapers have stronger and more rigid femoral and tibial
   midshafts than do `less-frequent' leapers. Landing style is conflated
   with leaping frequency and it is difficult to explain morphological
   variation exclusively by either leaping frequency or landing style.
   Further behavioral research, especially field study of locomotion, is
   needed to disentangle the effects of these mechanically important
   aspects of leaping locomotion.},
