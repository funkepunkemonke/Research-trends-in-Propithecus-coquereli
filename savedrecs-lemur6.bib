@article{ WOS:000086723300012,
Author = {Carvalho, LJM and Oliveira, SG and Alves, FA and Brigido, MCO and Muniz,
   JAPC and Daniel-Ribeiro, CT},
Title = {Aotus infulatus monkey is susceptible to Plasmodium falciparum infection
   and may constitute an alternative experimental model for malaria},
Journal = {MEMORIAS DO INSTITUTO OSWALDO CRUZ},
Year = {2000},
Volume = {95},
Number = {3},
Pages = {363-365},
Month = {MAY-JUN},
Abstract = {Aotus is one of the WHO-recommended primate models for studies in
   malaria, and several species can be infected with Plamodium falciparum
   or P. vivax. Here we describe the successful infection of the species A.
   infulatus from eastern Amazon with blood stages of P. falciparum. Both
   intact and splenectomized animals were susceptible to infection, the
   intact ones were able to keep parasitemias at lower levels for several
