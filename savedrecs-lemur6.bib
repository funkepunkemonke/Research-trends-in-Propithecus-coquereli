@article{ WOS:000185010900006,
Author = {Anapol, F and Gray, JP},
Title = {Fiber architecture of the intrinsic muscles of the shoulder and arm in
   semiterrestrial and arboreal guenons},
Journal = {AMERICAN JOURNAL OF PHYSICAL ANTHROPOLOGY},
