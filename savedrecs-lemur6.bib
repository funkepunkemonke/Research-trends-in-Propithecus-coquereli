@inproceedings{ WOS:000299901200296,
Author = {Gutierrez, Raquel and Fernandez, Samuel and Garcia, J. Jesus and Garcia,
   J. Carlos and Marnane, Liam},
Book-Group-Author = {IEEE},
Title = {Monitoring vital signs and location of patients by using ZigBee wireless
   sensor networks},
Booktitle = {2011 IEEE SENSORS},
Series = {IEEE Sensors},
Year = {2011},
Pages = {1221-1224},
Note = {10th IEEE Conference on Sensors, Limerick, IRELAND, OCT 28-31, 2011},
Organization = {IEEE; IEEE SENSORS Council; ANALOG DEVICES; Failte Ireland;
   irelandinspires.com; Silicon Labs; Sci Fdn Ireland (SFI); Intel;
   Competence Ctr; Microelect},
Abstract = {This work proposes a WWBAN (Wearable Wireless Body Area Network) for
   patients and its localization by means of a Zigbee-based LPS (Local
   Positioning System). To carry out both systems a mote has been used.
   This mote is a device composed by a microcontroller, some sensors and
   some communication modules. The WWBAN is used to measure user movement
   and heart rate, save it and send it to a remote device. For the LPS
