@inproceedings{ WOS:000238083200027,
Author = {Joho, Hideo and Jose, Joemon M.},
Editor = {Lalmas, M and MacFarlane, A and Ruger, S and Tombros, A and Tsikrika, T and Yavlinsky, A},
Title = {A comparative study of the effectiveness of search result presentation
   on the Web},
Booktitle = {ADVANCES IN INFORMATION RETRIEVAL},
Series = {Lecture Notes in Computer Science},
Year = {2006},
Volume = {3936},
Pages = {302-313},
