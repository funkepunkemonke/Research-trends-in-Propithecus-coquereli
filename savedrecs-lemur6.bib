Affiliation = {Mowry, CB (Corresponding Author), Berry Coll, Dept Biol, Mt Berry, GA 30149 USA.
   Berry Coll, Dept Biol, Mt Berry, GA 30149 USA.
   Duke Univ, Dept Biol Anthropol \& Anat, Durham, NC 27708 USA.},
DOI = {10.1023/B:IJOP.0000005984.36518.94},
ISSN = {0164-0291},
EISSN = {1573-8604},
Keywords = {indriids; feeding ecology; gut morphology; nutrition; secondary
   compounds},
Keywords-Plus = {RED COLOBUS MONKEYS; FOOD SELECTION; SEASONAL-VARIATION; FRUIT
   ABUNDANCE; PRIMATE; LEMUR; CERCOPITHECINES; CONSUMPTION; CHIMPANZEES;
