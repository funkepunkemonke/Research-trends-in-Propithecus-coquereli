@article{ WOS:000261821800010,
Author = {Veilleux, Carrie C. and Bolnick, Deborah A.},
Title = {Opsin Gene Polymorphism Predicts Trichromacy in a Cathemeral Lemur},
Journal = {AMERICAN JOURNAL OF PRIMATOLOGY},
