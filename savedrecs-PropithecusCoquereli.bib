@article{ WOS:000426818400028,
Author = {Guevara, Elaine E. and Frankel, David C. and Ranaivonasy, Jeannin and
   Richard, Alison F. and Ratsirarson, Joelisoa and Lawler, Richard R. and
   Bradley, Brenda J.},
Title = {A simple, economical protocol for DNA extraction and amplification where
   there is no lab},
Journal = {CONSERVATION GENETICS RESOURCES},
