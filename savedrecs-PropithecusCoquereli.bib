@article{ WOS:A1992JN78600005,
Author = {KUBZDELA, KS and RICHARD, AF and PEREIRA, ME},
Title = {SOCIAL-RELATIONS IN SEMIFREE-RANGING SIFAKAS (PROPITHECUS-VERREAUXI
   COQUERELI) AND THE QUESTION OF FEMALE DOMINANCE},
Journal = {AMERICAN JOURNAL OF PRIMATOLOGY},
Year = {1992},
Volume = {28},
Number = {2},
Pages = {139-145},
Abstract = {Female social dominance over males is thought to characterize most of
   the prosimian primates of Madagagascar. It has been reported in
   Propithecus verreauxi coquereli in the wild but intersexual relations
   were not fully characterized. In this paper we examine female-male
   spatial and agonistic relations in semi-free-ranging P. v. coquereli in
   order to evaluate intersexual social dominance and related behavioral
   asymmetries. Two hundred hours of focal sampling were conducted on two
