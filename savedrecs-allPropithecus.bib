@article{ WOS:000285754300025,
Author = {Norscia, Ivan and Palagi, Elisabetta},
Title = {Do wild brown lemurs reconcile? Not always},
Journal = {JOURNAL OF ETHOLOGY},
Year = {2011},
Volume = {29},
Number = {1},
Pages = {181-185},
Month = {JAN},
Abstract = {Post-conflict behaviour has been widely investigated in anthropoid
   primates but not extensively in prosimians. Here, we report the
   reconciliation pattern of a 14-individual group of wild brown lemurs
   (Eulemur fulvus) in the Berenty gallery forest (South Madagascar). We
   found that reconciliation occurs only in the non-feeding context and
   that it works in reducing the risk of renewed aggression. Thus,
   reconciliation would provide an immediate positive feedback that is
   probably independent of the quality of the relationship between
   opponents. Reconciliation may also be a hic-et-nunc mechanism, needed to
   avoid conflict spreading across group members, possibly leading to
   social disruption.},
