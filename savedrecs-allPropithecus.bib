@article{ WOS:A1994MY60600007,
Author = {GOODMAN, SM},
Title = {THE ENIGMA OF ANTIPREDATOR BEHAVIOR IN LEMURS - EVIDENCE OF A LARGE
   EXTINCT EAGLE ON MADAGASCAR},
Journal = {INTERNATIONAL JOURNAL OF PRIMATOLOGY},
Year = {1994},
Volume = {15},
Number = {1},
Pages = {129-134},
Month = {FEB},
Abstract = {Numerous zoologists who study diurnal lemurs on Madagascar have noted
   that they react strongly to the presence of birds of prey. For two of
   the most intensively studied lemurs, Propithecus verreauxi and Lemur
   catta, there are few documented cases of raptor predation. Thus, the
   maintenance of this stereotypic response is enigmatic. Bird bones
   recovered from cave surface deposits in southwestern Madagascar include
   the remains of an eagle (Aquila), a genus that has disappeared from
   Madagascar and that would have been capable of hunting animals the size
   of adult P. verreauxi and L. catta. The stereotypic response of these
   two lemurs toward raptors may have been retained from the period when
   this extinct eagle inhabited the island and is reinforced by rare acts
   of predation by extant birds of prey.},
